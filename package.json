{
  "name": "yield-rewind",
  "version": "1.0.0",
  "description": "High-performance refinery analytics dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "db:init": "ts-node scripts/init-db.ts",
    "db:seed": "ts-node scripts/seed-buckets.ts",
    "sync:yield": "python sync/sync-worker.py --type yield",
    "sync:sales": "python sync/sync-worker.py --type sales",
    "sync:tanks": "python sync/sync-worker.py --type tank",
    "sync:all": "python sync/sync-worker.py --type all",
    "sync:start": "ts-node sync/sync-service.ts",
    "backfill": "ts-node scripts/backfill-data.ts",
    "test:sqlite": "ts-node scripts/test-sqlite-performance.ts",
    "test:api": "curl -s http://localhost:3000/api/yield | head -c 500"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-popover": "^1.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-table": "^8.0.0",
    "@tanstack/react-virtual": "^3.0.0",
    "better-sqlite3": "^11.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.0",
    "echarts": "^5.5.0",
    "idb-keyval": "^6.2.0",
    "lucide-react": "^0.400.0",
    "next": "^15.0.0",
    "node-schedule": "^2.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20.0.0",
    "@types/node-schedule": "^2.1.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
